procedure TypeText(text : string);
var
  i : integer;

begin
  for i := 1 to Length(text) do
  begin
    KeyDown(GetKeyCode(text[i]));
    wait(RandomRange(50, 90));
    KeyUp(GetKeyCode(text[i]));
    wait(RandomRange(50, 400));
  end;


end;



